<h1>Users#edit</h1>
<%= form_for @user do |f| %>
	<%= f.label :fname %>
	<%= f.text_field :fname %>
	<%= f.label :lname %>
	<%= f.text_field :lname %>
	<%= f.label :birthday %>
	<%= f.date_field :birthday %>
	<%= f.label :street_address %>
	<%= f.text_field :street_address %>
	<%= f.label :state %>
	<%= f.text_field :state %>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<%= f.label :zip %>
	<%= f.text_field :zip %>
	<%= f.submit "Sign Up" %>
<% end %>
